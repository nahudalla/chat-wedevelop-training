{
  "name": "chat-wedevelop-training",
  "version": "1.0.0",
  "description": "WeDevelop Training Program Chat App",
  "private": true,
  "main": "src/server/index.js",
  "scripts": {
    "test": "standard && mocha --require @babel/register",
    "build-server": "mkdir -p dist/server && babel src/server dist/server",
    "build-server-watch": "mkdir -p dist/server && babel --watch src/server --out-dir dist/server",
    "start-server": "npm run build-server && nodemon --watch dist/server dist/server/index.js",
    "serve": "node dist/server/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nahudalla/chat-wedevelop-training.git"
  },
  "author": "Nahuel Dallacamina Ortea",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nahudalla/chat-wedevelop-training/issues"
  },
  "homepage": "https://github.com/nahudalla/chat-wedevelop-training#readme",
  "dependencies": {
    "sequelize": "^5.8.2",
    "sqlite3": "^4.0.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/register": "^7.4.4",
    "mocha": "^6.1.4",
    "nodemon": "^1.19.0",
    "standard": "^12.0.1"
  }
}
